/*
** Bootstrap over-rides
*/
body {
    font-family: 'Nunito Sans', sans-serif;
    color: #[gord_body_text];
    line-height: 1.5;
	font-size: 14px;
	background-color: #[gord_background_color];
}
h1 {
    font-size: 30px;
}
a{
	text-decoration: none !important;
}
body a {
    color: #[gord_text_link];
}
body a:hover {
    color: #[gord_text_link_hover];
}
.titles {
    margin: 0 0 20px;
    font-size: 18px;
    font-weight: 600;
    letter-spacing: 0;
	color: #[gord_title_headings];
}
.header, .widget-header, .page_heading, .qtyTable-btn{
	color: #[gord_title_headings];
}
.page_heading:after, .widget-header:after{
	border-bottom: 1px solid #[gord_title_headings];
}

@media(min-width: 1200px){
    .tc-animate-me{
        opacity: 0;
    }    
}

.cat-body, .products-section, .info-banner, .blog-section, .left-banner > div, .right-banner{
    overflow: hidden;
}

.subcategories {
    border-bottom: 1px solid #ececec;
    padding: 0 0 30px;
}
.subcategories > ul > li > a {
    padding: 10px 20px;
    color: #[gord_btn_text];
    background-color: #[gord_btn_bg];
    border: 1px solid #[gord_btn_border];
    text-decoration: none;
    display: block;
    -webkit-transition: all .25s ease-out;
    transition: all .25s ease-out;
}
.subcategories > ul > li > a:hover {
    border: 1px solid #[gord_btn_border_hover];
    color: #[gord_btn_text_hover] !important;
    background-color: #[gord_btn_bg_hover];

}
.subcategories > ul > li span.name {
    margin: 0;
    font-size: 14px;
    font-weight: 600;
    text-transform: uppercase;
}

.special-actions{
    margin-bottom: 20px;
}

.special-actions .special-action{
    width: 45%;
    margin-top: 10px;

}

.special-actions .special-action button{
    padding: 10px 20px;
    width: 100%;
    border: 1px solid #[gord_btn_border];
    color: #[gord_btn_text] !important;
    background-color: #[gord_btn_bg];
}
.special-actions .special-action button:hover{
    border: 1px solid #[gord_btn_border_hover];
    color: #[gord_btn_text_hover] !important;
    background-color: #[gord_btn_bg_hover];
    text-decoration: none;
}

@media(max-width: 767px){
    .special-actions .special-action{
        width: 100%; 
        margin: 10px 0 0;
    }
    .special-actions .special-action button{
        max-width: 350px;
    }
}  



@media (min-width: 1200px) {
    .container {
        width: 1200px;
    }
}
.form-control {
    color: #333;
    font-family: 'Nunito Sans', sans-serif;
    font-size: 13px;
    height: 36px;
    padding: 7px 12px;
}
.form-control.search-text {
     border:1px solid #ececec;
}

.btn {
    font-size: 13px;
    padding: 8px 12px;
    font-weight: 600;
    text-transform: uppercase;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
	
	color: #[gord_btn_text];
	border-width: 1px;
	border-style: solid;
	border-color: #[gord_btn_border];
	background-color: #[gord_btn_bg];
}
.btn-default,
.btn-primary:focus, 
.btn-primary:active:focus {
	color: #[gord_btn_text];
	border-width: 1px;
	border-style: solid;
	border-color: #[gord_btn_border];
	background-color: #[gord_btn_bg];
}
.btn-default:hover,
.btn-default:focus,
.btn-default:hover:focus,
.btn-default.focus {
	color: #[gord_btn_text_hover];
    background-color: #[gord_btn_bg_hover];
    border-color: #[gord_btn_border_hover];
}
.btn-default.btn-inverse,
.btn-primary {
	color: #[gord_btn_text];
	border-width: 1px;
	border-style: solid;
	border-color: #[gord_btn_border];
	background-color: #[gord_btn_bg];
}
.btn-default.btn-inverse:hover,
.btn-primary:hover {
    color: #[gord_btn_text_hover];
    background-color: #[gord_btn_bg_hover];
    border-color: #[gord_btn_border_hover];
    opacity: 0.8;
}
.navbar-wrapper{
	background-color: #[gord_nav_bg];
    box-shadow: 0 5px 5px rgba(66, 66, 66, 0.5);
	border-top: 1px solid #eee;
    position: relative;
    z-index: 999;
}
.navbar.navbar-inverse {
	position: absolute;
    margin-bottom: 0;
    border: none;
    background-color: #333;
}
.navbar-inverse .navbar-nav > li > a {
	position: relative;
    font-weight: 400;
    letter-spacing: 0.025em;
    text-transform: uppercase;
    font-size: 14px;
    line-height: 18px;
	padding: 10px 20px;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
	
	color: #[gord_nav_text];
	background-color: #[gord_nav_bg];
}
.navbar-inverse .navbar-nav > li > a:hover, .navbar-inverse .navbar-nav > li > a:focus {
	color: #[gord_nav_text_hover];
	background-color: #[gord_nav_bg_hover];
}
.dropdown-menu > li > a {
    font-size: 14px;
    padding: 10px 20px;
    text-transform: uppercase;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
	
	color: #[gord_nav_text];
	background-color: #[gord_nav_bg];
}
.dropdown-menu > li > a:hover{
    color: #[gord_nav_text_hover];
	background-color: #[gord_nav_bg_hover];
}
.navbar-nav > li.dropdown > a:after {
	content: "\f105";
    font-family: "fontello";
    font-size: 18px;
    line-height: 1;
    margin-left: 5px;
    position: absolute;
    right: 20px;
}

.dropdown-menu{
	position: absolute;
    top: 0;
    left: 100%;
    background-color: #333;
    min-width: 260px;
}

.navbar-nav > li{
	width: 100%;
}

.header-cat{
	position: relative;
}

.header-cat > a span {
    margin-right: 5px;
}

.head-cat-dropdown{
	position: absolute;
	width: 100%;
	display: none;
}

.header-cat > a{
    color: #fff;
    font-weight: 400;
    letter-spacing: 0.025em;
    text-transform: uppercase;
    font-size: 14px;
    line-height: 18px;
    min-height: 50px;
    display: block;
    padding: 15px 20px;
    background-color: #[gord_theme_color];
}

.header-cat > a:hover{
	color: #fff;
}

.header-cat > a:after{
	content: "\f107";
    font-family: "fontello";
    font-size: 18px;
    line-height: 1;
    margin-left: 20px;
}

.header-cat, #FRAME_MENU, #menulinks-outer{
	display: inline-block;
}

#menulinks {
	margin: 0;
}

#menulinks li{
	display: inline-block;
	text-transform: uppercase;
}

#menulinks li a{
	font-size: 14px;
	padding: 15px;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
	
	color: #[gord_nav_text];
	background-color: #[gord_nav_bg];
}

#menulinks li a:hover{
	color: #[gord_nav_text_hover];
	background-color: #[gord_nav_bg_hover];
}

.product-item .img .quickview{
    top: auto;
    width: 100%;
	
	color: #[gord_bloglist_atc_btn];
	border: 1px solid #[gord_bloglist_atc_border];
	background-color: #[gord_bloglist_atc_btn_bg];
}

.product-item .img .quickview:hover{
	color: #[gord_bloglist_atc_btn_hover];
	border: 1px solid #[gord_bloglist_atc_border_hover];
	background-color: #[gord_bloglist_atc_btn_bg_hover];
}

@media (max-width: 1023px) {
  .navbar-header {
      float: none;
  }
  .navbar-left,.navbar-right {
      float: none !important;
  }
  .navbar-toggle {
      display: block;
  }
  .navbar-collapse.collapse {
      display: none!important;
  }
  .navbar-nav {
      float: none!important;
  }
  .navbar-nav>li {
      float: none;
  }
  .collapse.in{
      display:block !important;
  }
  .navbar-nav .open .dropdown-menu {
        position: static;
        float: none;
        width: auto;
        margin-top: 0;
        background-color: transparent;
        border: 0;
        -webkit-box-shadow: none;
        box-shadow: none;
    }
    .navbar-inverse .navbar-nav .open .dropdown-menu > li > a {
        color: #9d9d9d;
    }
    .navbar-nav .open .dropdown-menu > li > a {
        line-height: 20px;
    }
    .navbar-nav .open .dropdown-menu > li > a, .navbar-nav .open .dropdown-menu .dropdown-header {
        padding: 5px 15px 5px 25px;
    }
}
/*
** Template code
*/
.logo {
    height: 115px;
    position: relative;
    overflow: hidden;
	text-align: center;
}
.logo img {
    position: absolute;
    top: 0;
    left: 0;
	right: 0;
    bottom: 0;
    margin: auto;
    max-width: 100%;
    max-height: 100%;
}
.logo a:hover img {
    opacity: 0.6;
}
.top-nav {
	font-size: 12px;
	padding: 10px 0;
	background-color: #[gord_theme_color];
	color: #fff;
	text-transform: uppercase;
}
.top-nav ul {
	padding: 5px 0 0;
    margin: 0;
    list-style: none;
    font-size: 14px;
    line-height: 1;
    font-family: 'Nunito Sans', sans-serif;
    text-transform: capitalize;
    font-weight: 400;
}
.top-nav ul li {
    float: left;
    margin-right: 10px;
    padding-right: 10px;
}
.useraccount{
	margin-left: 10px;
}
.top-nav .useraccount ul {
    float: right;
}
.top-nav ul li:last-child, .useraccount ul li{
    border: none;
    margin: 0;
    padding: 0;
}
.top-nav ul li a {
    color: #fff;
	font-size: 12px;
	text-transform: uppercase;
}

.top-nav ul li a:hover{
	color: #333;
	text-decoration: none;
}
.top-nav img{
	height: 20px;
}
.top-free-shipping span{
	vertical-align: bottom;
	margin-right: 5px;
}
.site-header {
    padding: 10px 0;
	background-color: #[gord_header_bg];
}

.searchBox {
    position: relative;
	margin-top: 25px;
    padding: 10px 0;
}
.search-form {
    position: relative;
	max-width: 458px;
    width: 100%;
    margin: auto;
}
.search-form .search-text {
    padding: 9px 62px 9px 12px;
    height: 45px;
	font-size: 14px;
	font-weight: 400;
	box-shadow: none;
	border-color: #e1e1e1;
}
.search-form .search-submit {
    position: absolute;
    top: 0;
    right: 0;
    width: 50px;
    height: 100%;
    text-align: center;
    font-size: 16px;
	background-color: #f6f4f4;
	border-color: #e1e1e1;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}

.search-form .search-submit:hover {
	opacity: 1;
}

.search-form .search-submit:hover img{
	filter: brightness(0) invert(1);
}

.search-form .search-submit svg path{
	fill: #[gord_theme_color];
}

.search-form .search-submit:hover svg path{
	fill: #ffffff;
}

.search-form-col div.searchlight-balloon{
    padding: 10px 0;
}

.right-header{
	text-align: right;
}

.wishlist-section, .shopping-cart, .account-section{
	display: inline-block;
}

.site-header .wishlist-section, .site-header .account-section{
	margin-right: 10px;
	padding: 10px 13px 9px;
}

.wishlist-section a, .account-section a{
	font-size: 18px; 
	color: #[gord_theme_color];
    padding: 0 5px;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}

.search-form .search-submit:hover{
	background-color: #[gord_theme_color];
}

.shopping-cart, .wishlist-section, .account-section{
	background-color: #f7f7f7;
    box-shadow: 0 0 2px #ccc;
    padding: 10px;
    margin-top: 35px;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}

.shopping-cart:hover, .wishlist-section:hover, .account-section:hover{
	background-color: #[gord_theme_color];
}

.wishlist-section:hover a, .account-section:hover a{
	color: #fff;
}

.shopping-cart:hover .shopping-cart-dropdown img{
	filter: brightness(0) invert(1);
}

.shopping-cart:hover .minicart-items{
	background-color: #fff;
}

.shopping-cart:hover .shopping-cart-dropdown svg path{
	fill: #ffffff;
}

.shopping-cart:hover .shopping-cart-dropdown:after{
	color: #fff;
}

.shopping-cart-dropdown svg path{
	fill: #[gord_theme_color];
}

.shopping-cart:hover .shopping-cart-dropdown .minicart-items{
	color: #[gord_theme_color];
}


.minicart {
    min-width: 165px;
    height: 60px;
    float: right;
    padding: 15px 0 15px 10px;
    border-left: 1px solid #ececec;
}
.minicart-inner {
    display: block;
    color: #333;
    text-transform: uppercase;
    cursor: pointer;
}
.minicart-inner:hover {
    color: #333;
    opacity: 0.8;
}

.shopping-cart-dropdown .minicart-items{
    position: absolute;
    top: -5px;
    right: 15px;
    height: 15px;
    width: 15px;
    border-radius: 10px;
    background-color: #[gord_theme_color];
    color: #fff;
    text-align: center;
    display: block;
    padding: 0px;
    font-size: 10px;
}

.minicart .cart-icon {
    display: block;
    width: auto;
    height: 30px;
    line-height: 30px;
    font-size: 30px;
    float: left;
    margin-right: 5px;
}
.minicart .cart-details {
    display: block;
    float: left;
}

.shopping-cart-dropdown{
	position: relative;
	height: 26px;
}

.shopping-cart-dropdown:after{
    content: "\f107";
    font-family: "fontello";
    font-size: 18px;
    line-height: 1;
    margin-left: 5px;
	color: #[gord_theme_color];
	vertical-align: middle;
}
span.cart-title {
    font-size: 12px;
    line-height: 15px;
    display: block;
    font-weight: bold;
}
span.cart-count {
    color: #666;
    font-size: 10px;
    display: block;
    line-height: 15px;
    font-weight: 500;
}
.site-footer {
    background-color: #[gord_footer_bg];
    padding: 55px 0 0;
    color: #[gord_footer_text];
    font-family: 'Nunito Sans', sans-serif;
}

.site-footer .logo {
	margin-bottom: 20px;
}

.site-footer .logo img{
	filter: brightness(0) invert(1);
}

.footer-social-media .social-icons{
    text-align: center;
}

.footer-menu{
	padding: 0;
}

.footer-about{
	font-size: 14px;
}

.social-icons{
	text-align: left;
}

ul.extrapages li {
    line-height: 1;
    margin: 0 0 15px;
}
ul.extrapages li a {
    color: #[gord_footer_link];
    font-size: 14px;
}

.footer-center ul.extrapages li a {
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}

.footer-center ul.extrapages li a:hover {
    padding-left: 5px;
    color: #[gord_footer_link_hover];
}

ul.extrapages li a:hover{
	color: #[gord_footer_link_hover];
}

.copyright ul.extrapages li{
	display: inline-block;
	margin: 0 20px 0 0;	
}

.copyright ul.extrapages li a:hover{
    color: #[gord_footer_link_hover];
}

.copyright ul.extrapages{
	text-align: right;
}

.extrapages{
	margin: 0;
	padding-left: 0;
}

.footer-social {
    border-top: 1px solid #2d2d2d;
    border-bottom: 1px solid #2d2d2d;
}
.social-icons,
.global-footer {
    padding-top: 15px;
    padding-bottom: 15px;
}
.copyright {
	margin-top: 50px;
    padding-top: 10px;
    padding-bottom: 10px;
    text-align: left;
    color: #[gord_footer_text];
    font-size: 12px;
    font-weight: 500;
	background-color: #[gord_footer_copyright_bg];
}
.copyright a {
    color: #fff;
}
#divRewardWidget {
    color: #333 !important;
}
.mailist-box {
    width: 100%;
    margin-bottom: 30px;
}
.mailist-box h3 {
	color: #[gord_footer_header_text];
    margin: 0 0 15px;
    /*font-family: 'Montserrat', sans-serif;*/
    font-family: 'Nunito Sans', sans-serif;
    font-size: 18px;
    text-transform: uppercase;
    font-weight: 600;
}

.mailinglist-form {
	position: relative;
	margin-top: 15px;
}
.mailinglist-input.form-control {
    background-color: #fff;
    border-color: #6b4e2a;
	border-radius: 25px;
    max-width: 530px;
	widh: 100%;
    color: #333;
    float: left;
	font-size: 16px;
	height: 45px;
}
.mailinglist-submit {
	position: absolute;
    top: 3px;
    right: 4px;
    background-color: #[gord_theme_color];
    color: #fff !important;
    border-color: #fff !important;
    float: right;
    height: 39px;
	padding: 5px 25px;
    font-size: 14px;
    text-transform: uppercase;
    border-radius: 25px;
}
.btn-default.mailinglist-submit:hover, 
.btn-default.mailinglist-submit:focus {
	background-color: #[gord_theme_color];
	opacity: .7;
}
.mailinglist-response {
    display: none;
}
.maillist-options {
	display: none;
    margin: 15px 0 0;
}
.subscribe {
    float: left;
    margin-right: 20px;
    line-height: 13px;
}
.subscribe input[type="radio"] {
    margin: 0;
    vertical-align: middle;
}
.subscribe label {
    margin: 0;
    font-size: 12px;
    line-height: 13px;
    font-weight: 300;
    margin-left: 3px;
    color: #ccc;
    cursor: pointer;
}

.footer-social ul {
    margin: 0;
}
ul.payment-icons {
    text-align: center;
}
ul.payment-icons li {
    font-size: 34px;
