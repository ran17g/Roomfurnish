    content: "\e803 \e803 \e803 \e803 \e803";
    color: #b8b8b8;
}
.rating:hover .rating-star:hover,
.rating:hover .rating-star:hover ~ .rating-star,
.rating-input:checked ~ .rating-star,
.reviews-stars {
    color: #[gord_stars];
	letter-spacing: 3px;
}

.product-item .reviews span.reviews-count{
	display: none;
}

.product-items .product-item{
	color: #[gord_fp_text];
}

.product-item .name a {
    color: #[gord_bloglist_product_name];
	font-size: 16px;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}

.product-item:hover .name a {
	color: #[gord_bloglist_product_name_hover];
}

.product-item del.regular-price {
    text-decoration: line-through;
}
.sale-price {
    color: #[gord_theme_color];
}
del.original-price {
    font-weight: 500;
}
span.freeshipping {
    color: #6f4125;
}
.product-item .status span:before {
    content: "-";
}
.product-item .status {
    font-size: 12px;
}
.product-item .price {
    font-size: 16px;
    font-weight: bold;
}

.product-item .img {
    min-height: 304px;
    position: relative;
    border: 1px solid #eee;
    background-color: #fff;
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
    transition: all .3s ease-in-out;
}

.product-items .product-item:hover .img{
    box-shadow: 0 0 10px #eee;
